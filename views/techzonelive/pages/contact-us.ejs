    <script>
      $(document).on("click", "#submit-from", function(){
         console.log("Yes you clicked me!");
         var userName = $("#userName").val();
         var userEmailId = $("#userEmailId").val();
         var userPhone = $("#phone").val();
         var userMessage = $("#userMessage").val();
         // console.log(userName)
         // console.log(userEmailId)
         // console.log(userPhone)
         // console.log(userMessage)

         if(userName == ""){
            $("#errMsg").html("**Enter User Name");
            $("#userName").addClass("border border-danger");
         }else{
            $("#errMsg").html("");
            $("#userName").removeClass("border border-danger");
            if(userName.length >= 30){
               $("#errMsg").html("**Name leangth should lesser then 30!");
               $("#userName").addClass("border border-danger");
            }else{
               $("#errMsg").html("");
               $("#userName").removeClass("border border-danger");
               if(!isNaN(userName)){
                  $("#errMsg").html("**Please enter valid Name!");
                  $("#userName").addClass("border border-danger");
               }else{
                  $("#errMsg").html("");
                  $("#userName").removeClass("border border-danger");
                  if(userEmailId == ""){
                        $("#errMsg").html("**Please Enter Email!");
                        $("#userEmailId").addClass("border border-danger");
                    }else{
                        $("#errMsg").html("");
                        $("#userEmailId").removeClass("border border-danger");
                        if(userEmailId.indexOf('@') <= 0){
                           $("#errMsg").html("**Invalid '@' position!");
                           $("#userEmailId").addClass("border border-danger");
                        }else{
                           $("#errMsg").html("");
                           $("#userEmailId").removeClass("border border-danger");
                            if((userEmailId.charAt(userEmailId.length-4) != ".") && (userEmailId.charAt(userEmailId.length-3) != ".")){
                              $("#errMsg").html("**Invalid '.' position!");
                              $("#userEmailId").addClass("border border-danger");
                            }else{
                              $("#errMsg").html("");
                              $("#userEmailId").removeClass("border border-danger");
                              if(userMessage == ""){
                                 $("#errMsg").html("**Enter your message please");
                                 $("#userMessage").addClass("border border-danger");
                              }else{
                                 swal({
                                    title: "Submitted!",
                                    text: "Your form has been submitted!",
                                    icon: "success",
                                    dangerMode : false,
                                    button: "Ok",
                                 }).then(function(success){
                                    if(success){
                                       location.reload();
                                    }
                                 });
                              }
                            }
                           }
                        }
                  

               }
            }
         }


      })
    </script>
